<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sweetlime.tethys"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="19" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.tethys_1.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.tethys_1.College"
            android:label="@string/app_name" />
            <activity
            android:name="com.example.tethys_1.AutoSelect"
            android:label="@string/app_name"
            android:launchMode="singleTop" />
            <activity
            android:name="com.example.tethys_1.Listbooks"
            android:label="@string/app_name" />
            <activity  android:name="com.example.tethys_1.eachbook" />
             <activity  android:name="com.example.tethys_1.Manual" />
              <activity  android:name="com.example.tethys_1.Checkout" />
              <activity android:name="com.example.tethys_1.NavigationActivity" />
    </application>
 <uses-permission android:name="android.permission.INTERNET"/>
</manifest>
